#include<stdio.h>
int main()
{
	int year;
	printf("enter a year");
	scanf("%d",&year);
	// leap year is perfectly divisible by 400
if (year%400==0)
	{
		printf("%d is a leap year",year);
	}
		// not leap year divisible by 100
		//but not divisible with 400
else if(year%100==0){

		printf("%d is not a  leap year",year);
	}
	//leap year if not divisible by 100
	//but divisible by 4
	else if(year%4==0)
	{
		printf("%d is  a leap year",year);
	}
	//all others years are not a leap year
	else{
		printf("%d is not a leap year",year);
	}
}
