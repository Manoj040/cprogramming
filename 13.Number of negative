#include<stdio.h>
int main()
{
	int a[100],i,n ;
	printf("enter the size of an arry");
	scanf("%d",&n);
	printf("enter the elements in arry");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("number of negative numbers :");
	for(i=0 ;i<n ;i++)
	{
		if(a[i]<0)
		{
			printf("%d\t",a[i]);
		}
	}
	return 0;
	}
